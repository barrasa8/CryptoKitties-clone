  "source": "pragma solidity 0.5.12;\n\nimport \"@openzeppelin/upgrades/contracts/Initializable.sol\";\n\ncontract Ownable is Initializable{\n    address public owner;\n\n    modifier onlyOwner(){\n        require(msg.sender == owner);\n        _; //Continue execution\n    }\n\n    constructor() public  {\n        owner = msg.sender;\n    }\n}",
